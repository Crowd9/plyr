{
    "name": "plyr",
    "version": "3.5.6",
    "description": "A simple, accessible and customizable HTML5, YouTube and Vimeo media player",
    "homepage": "https://plyr.io",
    "author": "Sam Potts <sam@potts.es>",
    "main": "dist/plyr.js",
    "module": "dist/plyr.min.mjs",
    "jsnext:main": "dist/plyr.min.mjs",
    "browser": "dist/plyr.min.js",
    "sass": "src/sass/plyr.scss",
    "style": "dist/plyr.css",
    "keywords": [
        "HTML5 Video",
        "HTML5 Audio",
        "Media Player",
        "DASH",
        "Shaka",
        "WordPress",
        "HLS"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/sampotts/plyr.git"
    },
    "bugs": {
        "url": "https://github.com/sampotts/plyr/issues"
    },
    "browserslist": "> 1%",
    "scripts": {
        "build": "gulp build && postcss demo/dist/demo.css -r",
        "lint": "eslint src/js && npm run-script remark",
        "lint:fix": "eslint --fix src/js",
        "remark": "remark -f --use 'validate-links=repository:\"sampotts/plyr\"' '{,!(node_modules),.?**/}*.md'",
        "deploy": "yarn lint && gulp deploy"
    },
